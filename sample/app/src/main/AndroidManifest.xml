<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.activitystarter">

    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:label="@string/app_name"
        tools:ignore="MissingApplicationIcon,UnusedAttribute">
        <activity
            android:name="com.example.activitystarter.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.activitystarter.StudentDataActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.example.activitystarter.StudentParcelableActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.example.activitystarter.StudentSerializableActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>